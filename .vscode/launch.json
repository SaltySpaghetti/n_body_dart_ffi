    "version": "0.2.0",
    "configurations": [
        {
            "name": "n_body_dart_ffi",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "n_body_dart_ffi (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "n_body_dart_ffi (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "Debug native Linux",
            "preLaunchTask": "compile linux debug",
            "type": "cppdbg",
            "request": "launch",
            "args": [],
            "program": "${workspaceFolder}/build/linux/x64/debug/bundle/n_body_dart_ffi",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Debug native Windows",
            "preLaunchTask": "compile windows debug",
            "type": "cppvsdbg",
            "request": "launch",
            "args": [],
            "program": "${workspaceFolder}/build/windows/runner/Debug/n_body_dart_ffi.exe",
            "cwd": "${workspaceFolder}"
        }
    ]
}